	$(AM_V_CCLD)$(libdirectfb_mesa_system_la_LINK) -rpath $(systemsdir) $(libdirectfb_mesa_system_la_OBJECTS) $(libdirectfb_mesa_system_la_LIBADD) $(LIBS)

