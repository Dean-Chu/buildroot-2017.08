installdirs:
	for dir in "$(DESTDIR)$(systemsdir)" "$(DESTDIR)$(systemsdir)" "$(DESTDIR)$(internalincludedir)"; do \
	  test -z "$$dir" || $(MKDIR_P) "$$dir"; \
	done
